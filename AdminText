while wait(1) do
     pcall(function()
     if game.Players:FindFirstChild("hhshahfhfhahsh") then

            if not game.Players["hhshahfhfhahsh"].Character.Head:FindFirstChild("ESP") then

local Texts = "Blackking-Owner"
local BillboardGui = Instance.new("BillboardGui")
local TextLabel = Instance.new("TextLabel")
